<div class='frost-application-bar'>
  {{#yield-slot 'logo'}}
    {{yield}}
  {{/yield-slot}}
  {{#each categories as |category|}}
    {{nav-category
      icon=category.icon
      name=category.name
      pack=category.pack
    }}
  {{/each}}
